{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -k -c auto -n web,api,gen  \"npm run dev:web\" \"npm run dev:api\" \"npm run dev:codegen\"",
    "dev:web": "next dev --turbopack web",
    "dev:api": "dotnet watch run --project Api/Api.csproj",
    "dev:codegen": "APP_URL=http://localhost:3000 orval --watch",
    "build": "next build web",
    "start": "next start web",
    "lint": "next lint web",
    "prepare": "husky"
  },
  "dependencies": {
    "next": "15.1.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "eslint-config-prettier": "^10.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "orval": "^7.4.1",
    "postcss": "^8",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "lint-staged": {
    "web/**/*": "prettier --write --ignore-unknown"
  }
}
