{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -- concurrently -k -c auto -n web,api,gen,mail \"npm run dev:web\" \"npm run dev:api\" \"npm run dev:codegen\" \"npm run maildev\"",
    "dev:web": "next dev --turbopack web",
    "dev:api": "dotnet watch run --project Api/Api.csproj",
    "dev:codegen": "dotenv -- orval --watch",
    "build": "next build web",
    "maildev": "maildev --smtp 1025 --web 5000",
    "start": "next start web",
    "lint": "next lint web",
    "prepare": "husky"
  },
  "dependencies": {
    "next": "15.1.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-toastify": "^11.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "dotenv": "^16.4.7",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "eslint-config-prettier": "^10.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "maildev": "^2.2.1",
    "orval": "^7.4.1",
    "postcss": "^8",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "lint-staged": {
    "web/**/*": "prettier --write --ignore-unknown"
  }
}
